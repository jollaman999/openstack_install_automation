default openstack
label openstack

kernel vmlinuz

append initrd=initrd ISCSI_INITIATOR=OPENSTACK_AUTO_INSTALL_ISCSI_TARGET_NAME ISCSI_TARGET_NAME=OPENSTACK_AUTO_INSTALL_ISCSI_TARGET_NAME ISCSI_TARGET_IP=OPENSTACK_AUTO_INSTALL_ISCSI_TARGET_IP ISCSI_TARGET_PORT=3260 root=/dev/disk/by-uuid/OPENSTACK_AUTO_INSTALL_ISCSI_OS_VOLUME_ROOT_UUID rw ip=dhcp console=ttyS0,115200n8 rdshell
